@model Skill
@{
    ViewData["Title"] = "UpdateSkill";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}
<h2>Yetenek Güncelleme</h2>
<form method="post">
    <input type="Hidden" asp-for="SkillId" class="form-control">
    <label>Yetenek Başlığı</label>
    <input type="text" asp-for="SkillName" class="form-control">
    <br />
    <label>Yetenek Değeri</label>
    <input type="number" asp-for="SkillValue" class="form-control">
    <br />
    <button class="btn btn-outline-success">Yeteneği Güncelle</button>
</form>