@model About
@{
    ViewData["Title"] = "AddAbout";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h1>Hakkımda Bilgisi Ekleme Sayfası</h1>

<form method="post">
    <label>Açıklama</label>
    <input type="text" class="form-control" asp-for="Description" />
    <br />
    <label>Alan</label>
    <input type="text" class="form-control" asp-for="JobTitle" />
    <br />
    <label>Görsel Yolu</label>
    <input type="url" class="form-control" asp-for="ImageURL" />
    <br />
    <label>Mail Adresi</label>
    <input type="text" class="form-control" asp-for="Email" />
    <br />
    <label>Telefon Numarası</label>
    <input type="text" class="form-control" asp-for="Phone" />
    <br />
    <label>Şehir</label>
    <input type="text" class="form-control" asp-for="City" />
    <br />
    <label>Alt Açıklama</label>
    <input type="text" class="form-control" asp-for="SubDescription" />
    <br />
    <button class="btn btn-outline-info">Ekle</button>
</form>